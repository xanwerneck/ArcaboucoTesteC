=declararparm	aresAB	 string  "AB"
=declararparm	aresBA	 string  "BA"
=declararparm	aresCA	 string  "CA"
=declararparm	aresAD	 string  "AD"
=declararparm	aresAE	 string  "AE"
=declararparm	aresEC	 string  "EC"
