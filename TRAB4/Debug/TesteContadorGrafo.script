==Criar Aresta DE
=criararesta		casa3	 casa4	grafo1	aresDE	OK	
==Criar Aresta GD
=criararesta		casa6	 casa3	grafo1	aresGD	OK

== Recria o grafo2
=criargrafo  grafo2 OK

//* Verificações finais */

== Exibir todos os espaços
=exibirtodosespacos     EspTodos

== Eliminar todos os espacos alocados
